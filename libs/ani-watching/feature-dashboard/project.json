{
  "name": "ani-watching-feature-dashboard",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ani-watching/feature-dashboard/src",
  "prefix": "zjk",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ani-watching/feature-dashboard/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/ani-watching/feature-dashboard/**/*.ts",
          "libs/ani-watching/feature-dashboard/**/*.html"
        ]
      }
    }
  },
  "tags": ["feature"]
}
